{
    "name": "janole/laravel-dockerize",
    "version": "0.1.3",
    "description": "A very simple tool to containerize your Laravel app",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Ole Suhr",
            "email": "ole@janole.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "symfony/yaml": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "janole\\Laravel\\Dockerize\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "janole\\Laravel\\Dockerize\\PackageServiceProvider"
            ]
        }
    }
}
